CAPPEO ENTRE 0 Y 1 :
=IF(A1 < 0, 0, IF(A1 > 1, 1, A1))

VARIOS IF:
=IFS(A2="CN",75%,A2="D1",10%,A2="D2",5%,A2="U1",10%)




SELECT COLUMN_NAME, DATA_TYPE, CHARACTER_MAXIMUM_LENGTH
FROM OPENQUERY([CARM], 'SELECT COLUMN_NAME, DATA_TYPE, CHARACTER_MAXIMUM_LENGTH
                        FROM INFORMATION_SCHEMA.COLUMNS
                        WHERE TABLE_NAME = ''GCSECUP''
                        AND COLUMN_NAME = ''SECUHEFIND''')




import pyautogui
import time
import os

# Ruta para abrir Microsoft Teams (puedes ajustarla según tu sistema)
teams_path = r"C:\Program Files (x86)\Microsoft\Teams\current\Teams.exe"

# Abrir Microsoft Teams
os.startfile(teams_path)

# Esperar a que Teams se abra (ajusta el tiempo según sea necesario)
time.sleep(30)

while True:
    # Escribir la letra "A"
    pyautogui.write("A")
    
    # Esperar 5 minutos
    time.sleep(300)
